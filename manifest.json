{
  "manifest_version": 3,
  "name": "World Time Zone Map",
  "version": "1.0",
  "description": "Displays a world map with time zones on Chrome's new tab page",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon16.png"
  }
}